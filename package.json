{
  "name": "yuedun_blog",
  "version": "1.0.0",
  "description": "关于dependencies符号的解释：1.version 必须完全和version一致   2.>version 必须比version大 3.>=version 同上  4.<version 同上  5.<=version 同上  6.~version 大约一样  7. 1.2.x 1.2.0, 1.2.1, 等，但不包括1.3.0  8.^1.2.1大于1.2.1小于2.0.0",
  "private": true,
  "scripts": {
    "tsc": "tsc -w --pretty",
    "serve": "set NODE_ENV=development& set DEBUG=yuedun:*& cd ./build & npx nodemon --delay 5 ./bin/www.js",
    "dev": "parallelshell \"npm run tsc\" \"npm run serve\"",
    "start": "node ./bin/www.js"
  },
  "dependencies": {
    "async": "^3.2.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "connect": "^3.7.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "debug": "^4.3.1",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "formidable": "^1.2.2",
    "lodash": "^4.17.20",
    "markdown-it": "^11.0.1",
    "moment": "^2.29.1",
    "mongoose": "^5.11.8",
    "morgan": "^1.10.0",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.4.17",
    "qiniu": "^7.3.2",
    "request": "^2.88.2",
    "serve-favicon": "^2.4.5",
    "utility": "^1.17.0"
  },
  "devDependencies": {
    "@types/async": "^3.2.5",
    "@types/bluebird": "^3.5.33",
    "@types/debug": "^4.1.5",
    "@types/express-session": "^1.17.3",
    "@types/formidable": "^1.0.31",
    "@types/lodash": "^4.14.165",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.10.2",
    "@types/node": "^14.14.14",
    "@types/nodemailer": "^6.4.0",
    "@types/request": "^2.48.5",
    "@types/typescript": "^2.0.0",
    "gulp": "^4.0.2",
    "parallelshell": "^3.0.2",
    "source-map-support": "^0.5.19",
    "typescript": "^3.9.7"
  }
