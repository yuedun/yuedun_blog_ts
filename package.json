{
  "name": "nodejs-blog-ts",
  "version": "1.0.0",
  "description": "关于dependencies符号的解释：1.version 必须完全和version一致   2.>version 必须比version大 3.>=version 同上  4.<version 同上  5.<=version 同上  6.~version 大约一样  7. 1.2.x 1.2.0, 1.2.1, 等，但不包括1.3.0  8.^1.2.1大于1.2.1小于2.0.0",
  "private": true,
  "scripts": {
    "tsc": "tsc -w --pretty",
    "serve": "set NODE_ENV=development& set DEBUG=yuedun:*& cd ./build & nodemon --delay 5 ./bin/www.js",
    "dev": "parallelshell \"npm run tsc\" \"npm run serve\"",
    "start": "node ./bin/www.js"
  },
  "dependencies": {
    "async": "^0.9.2",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "connect": "^3.7.0",
    "connect-mongo": "^0.8.2",
    "cookie-parser": "^1.4.4",
    "debug": "^2.6.9",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "formidable": "^1.2.1",
    "lodash": "^4.17.11",
    "markdown-it": "^6.1.1",
    "moment": "^2.24.0",
    "mongoose": "^4.13.18",
    "morgan": "^1.9.1",
    "node-schedule": "^1.3.2",
    "nodemailer": "^2.7.2",
    "qiniu": "^6.1.13",
    "request": "^2.88.0",
    "serve-favicon": "^2.4.5",
    "utility": "^1.16.1"
  },
  "devDependencies": {
    "@types/async": "^2.4.2",
    "@types/bluebird": "^3.5.27",
    "@types/debug": "^4.1.4",
    "@types/ejs": "^2.6.3",
    "@types/express-session": "0.0.32",
    "@types/formidable": "^1.0.31",
    "@types/lodash": "^4.14.136",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^4.7.46",
    "@types/node": "^8.10.50",
    "@types/nodemailer": "^4.6.8",
    "@types/request": "^2.48.1",
    "@types/typescript": "^2.0.0",
    "gulp": "^3.9.1",
    "parallelshell": "^3.0.2",
    "source-map-support": "^0.4.18",
    "typescript": "^2.9.2"
  }
}
